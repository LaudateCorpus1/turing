{
  "name": "room",
  "version": "0.2.0",
  "description": "node.js and browser client for roomdb",
  "main": "build/room.js",
  "repository": "https://github.com/jedahan/room",
  "author": "Jonathan Dahan",
  "license": "MIT",
  "devDependencies": {
    "@jedahan/room-server": "jedahan/room-server",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.54.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "roomdb": "alexwarth/roomdb#modularify",
    "serve": "^6.4.8"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "node -e 'require(\"@jedahan/room-server/server.js\")'",
    "assert": "node examples/cli.js assert",
    "retract": "node examples/cli.js retract",
    "select": "node examples/cli.js select",
    "examples": "serve --cors examples",
    "systemd": "npm-run-all systemd:remove systemd:add",
    "systemd:add": "NODE=`which node` sudo -E bash -c '$NODE service.js add'",
    "systemd:remove": "NODE=`which node` sudo -E bash -c '$NODE service.js remove'"
  },
  "dependencies": {
    "node-fetch": "^1.7.3",
    "room-server": "jedahan/room-server"
  }
}
